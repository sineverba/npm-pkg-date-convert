{
  "name": "@sineverba/date-convert",
  "version": "2.3.0",
  "description": "Convert a date from / to ISO format YYYYMMDD to human readable format (DD/MM/YYYY)",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sineverba/npm-pkg-date-convert.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "tslint -p tsconfig.json",
    "prettier": "prettier -c \"src/**/*.ts\"",
    "fixprettier": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "npm",
    "package",
    "date",
    "format"
  ],
  "author": "alessandro.arici@gmail.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "coveralls": "^3.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-cucumber": "^3.0.1",
    "npm-check-updates": "^16.14.15",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**/*"
  ]
